{
  "env": {
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "brace-style": "off",
    "quotes": "off",
    "jsx-quotes": "off",
    "linebreak-style": "off",
    "react/jsx-props-no-spreading": "off"
  }
}
