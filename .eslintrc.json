{
  "env": {
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "linebreak-style": "off",
    "quotes": "off",
    "jsx-quotes": "off",
    "brace-style": "off",
    "react/jsx-props-no-spreading": "off"
  }
}
